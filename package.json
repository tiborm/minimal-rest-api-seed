{
  "name": "metron-middle-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc -w",
    "start": "nodemon dist/server",
    "start-watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node - Express\" -c \"cyan.bold,green.bold\" \"npm run build-watch\" \"npm start\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.1",
    "@types/morgan": "^1.7.37",
    "concurrently": "^5.0.0",
    "nodemon": "^1.19.4",
    "typescript": "^3.6.4"
  }
}
