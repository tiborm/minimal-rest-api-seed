{
  "name": "metron-middle-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc -w",
    "start": "nodemon dist/server",
    "start-watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node - Express\" -c \"cyan.bold,green.bold\" \"npm run build-watch\" \"npm start\""
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.2"
  }
}
